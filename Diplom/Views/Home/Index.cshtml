@model Diplom.Models.ViewModels.HomeListsViewModel
@{
    ViewData["Title"] = "Home Page";
}

<section id="slider-home-v4" class="slider-04">
    <h2 class="none">Slider</h2>
    <div class="revolution_slider">
        <div class="r_slider fixheightmb" data-width="1920" data-height="700" data-screen="off" data-fullwidth="on">
            <ul>
                <li class="f_left" data-transition="curtain-1" data-slotamount="7">
                    <img src="~/images/slider/home-04.jpg" alt="slider" data-bgrepeat="no-repeat" data-bgfit="cover" data-bgposition="center center">
                    <div class="caption sfl str rs-parallaxlevel-4" data-x="707" data-y="180" data-speed="500" data-start="500" style="font-size: 40px;">
                        <span class="saleoff rsaleo">Sale off</span>
                    </div>
                    <div class="caption sfl str tt_uppercase rs-parallaxlevel-4" data-x="707" data-y="235" data-speed="800" data-start="500">
                        <img class="sale75" src="images/slider/sale75-home4.png" alt="">
                    </div>
                    <div class="caption sfl str rs-parallaxlevel-6"
                         data-x="620" data-y="410" data-speed="500" data-start="1500"
                         style="z-index: 10;font-size: 66px; letter-spacing:2px; line-height: 80px;">
                        <span class="friday">Black Friday</span>
                    </div>
                    <div class="caption sfl str rs-parallaxlevel-3" data-x="690" data-y="490" data-speed="500" data-start="1500"
                         style="z-index: 10;font-size: 26px; letter-spacing:5px;">
                        <span class="low-prices">Unbelievable Low Prices</span>
                    </div>
                </li>
                <li class="f_left" data-transition="random" data-slotamount="7">
                    <img src="~/images/slider/home-v4-02.jpg" alt="slider" data-bgrepeat="no-repeat" data-bgfit="cover" data-bgposition="center center">
                    <div class="caption sfr stl rs-parallaxlevel-6"
                         data-x="580" data-y="195" data-speed="500" data-start="1500" style="z-index: 9;">
                        <img class="mb-img-home4-01" src="~/images/slider/img-text-hom4-v2.png" alt="">
                    </div>
                    <div class="caption sfr str rs-parallaxlevel-4"
                         data-x="400"
                         data-y="0"
                         data-speed="500"
                         data-start="500"
                         data-easing="Power4.easeOut"
                         style="z-index: 11;">
                        <img class="mb-img-home4-02" src="~/images/slider/img-home4.png" alt="home4">
                    </div>
                </li>
            </ul>
        </div>
    </div>
</section>
